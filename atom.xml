---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Og Maciel</title>
 <link href="http://www.ogmaciel.com/atom.xml" rel="self"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.ogmaciel.com/</id>
 <author>
   <name>Og Maciel</name>
   <email>omaciel@ogmaciel.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.ogmaciel.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.ogmaciel.com/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
