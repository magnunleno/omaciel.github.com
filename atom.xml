---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

   <title>Og Maciel</title>
   <link href="https://omaciel.github.io" />
   <link type="application/atom+xml" href="https://omaciel.github.io/atom.xml" rel="self"/>
   <updated>{{ site.time | date_to_xmlschema }}</updated>
   <id>https://omaciel.github.io/</id>
   <author>
     <name>Og Maciel</name>
     <email>omaciel@ogmaciel.com</email>
   </author>

   {% for post in site.posts %}
   <entry>
     <id>https://omaciel.github.io{{ post.id }}</id>
     <link href="https://omaciel.github.io{{ post.url }}"/>
     <title>{{ post.title }}</title>
     <published>{{ post.date | date_to_xmlschema }}</published>
     <updated>{{ post.date | date_to_xmlschema }}</updated>
     <author>
       <name>Og Maciel</name>
       <uri>https://omaciel.github.io/</uri>
     </author>
     <content type="html">{{ post.content | xml_escape }}</content>
   </entry>
   {% endfor %}

</feed>
